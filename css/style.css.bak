@charset "utf-8";
/* CSS Document */
/* Sticky footer styles
-------------------------------------------------- */
html {
  position: relative;
  min-height: 100%;
}
body {
  /* Margin bottom by footer height */
  margin-bottom: 60px;
}
.footer {
  position: absolute;
  bottom: 0;
  width: 100%;
  /* Set the fixed height of the footer here */
  height: 60px;
  line-height: 60px; /* Vertically center the text there */
  background-color: #e3f2fd;
}


/* Custom page CSS
-------------------------------------------------- */
/* Not required for template or sticky footer method. */

body > .container {
  padding: 60px 15px 0;
}

.footer > .container {
  padding-right: 15px;
  padding-left: 15px;
}

code {
  font-size: 80%;
}
.img-fluid{
	max-width: 100%;
	height: auto;
}
.art-header
{
   -webkit-border-radius:18px;
   -moz-border-radius:18px;
   border-radius:18px;
   margin:0 auto;
   background-repeat: no-repeat;
   height: 250px;
   background-image: url('images/object25411573.png'),url('images/object1092673980.jpg'), url('images/header.jpg');
   background-position: 2px -2px,-54px -253px, 0 0;
   position: relative;
   z-index: auto !important;
}
